      PLANT_GEN_MOVEMENT_ID["üÜî Generar movement_id<br>movement_id = gen_random_uuid()"]
      PLANT_INSERT_MOVEMENT@{ label: "üìä INSERT INTO stock_movements<br>movement_id = UUID<br>batch_id = NULL<br>movement_type = 'plantar'<br>source_bin_id = NULL<br>destination_bin_id = bin_id<br>quantity = QUANTITY<br>user_id = USER_ID<br>unit_price = NULL<br>total_price = NULL<br>reason_description = NOTES<br>processing_session_id = NULL<br>source_type = 'manual'<br>is_inbound = true<br>created_at = NOW<br>RETURNING id AS movement_id_pk" }
