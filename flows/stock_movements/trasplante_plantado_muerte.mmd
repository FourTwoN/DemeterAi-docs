      TRANS_COMMIT["‚úÖ COMMIT Transaction"]
      TRANS_BUILD_RESPONSE["üì¶ Build response<br>source_movement_id: UUID<br>dest_movement_id: UUID<br>source_batch_remaining: int<br>dest_batch_total: int<br>quantity_moved: int<br>operation: 'transplante'"]
      TRANS_END(["‚úÖ Service SUCCESS"])
  end

