      TRANS_ERROR_INSUFFICIENT["üö® ERROR 400<br>Insufficient stock<br>Available: X, Requested: Y"]
      TRANS_FIND_DEST_BIN[("üìä SELECT sb.id<br>FROM storage_bins sb<br>WHERE sb.storage_location_id = dest_id<br>AND sb.status = 'active'<br>ORDER BY created_at DESC<br>LIMIT 1")]
