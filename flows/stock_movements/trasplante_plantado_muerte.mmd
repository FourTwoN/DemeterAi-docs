      TRANS_UPDATE_DEST_BATCH@{ label: "üìä UPDATE stock_batches<br>SET quantity_current = quantity_current + QUANTITY<br>updated_at = NOW<br>WHERE id = dest_batch_id" }
      TRANS_LINK_MOVEMENTS[("üìä UPDATE stock_movements<br>SET batch_id = dest_batch_id<br>WHERE id = mov_in_pk<br>‚ö†Ô∏è Link IN movement to dest batch")]
      TRANS_VERIFY["‚úÖ COMPREHENSIVE VERIFICATION<br>‚ö†Ô∏è Source decreased correctly<br>‚ö†Ô∏è Dest increased correctly<br>‚ö†Ô∏è Both movements linked<br>‚ö†Ô∏è Total quantity conserved"]
      TRANS_ALL_VALID{"Todo<br>v√°lido?"}
      TRANS_ROLLBACK["üîÑ ROLLBACK COMPLETO<br>DELETE movements<br>REVERT batch updates<br>Log error con traceback"]
