# DemeterAI v2.0 - Environment Variables Template
# Copy to .env and fill in actual values

# Database (PostgreSQL 18)
DATABASE_URL=postgresql+asyncpg://user:password@localhost:5432/demeterai
DATABASE_URL_SYNC=postgresql+psycopg://user:password@localhost:5432/demeterai

# Redis (Celery broker + result backend)
REDIS_URL=redis://localhost:6379/0
CELERY_BROKER_URL=redis://localhost:6379/0
CELERY_RESULT_BACKEND=redis://localhost:6379/1

# AWS S3
AWS_ACCESS_KEY_ID=your_access_key_here
AWS_SECRET_ACCESS_KEY=your_secret_key_here
AWS_S3_BUCKET=demeterai-photos
AWS_REGION=us-east-1

# JWT Authentication
JWT_SECRET_KEY=change_this_to_random_secret_in_production
JWT_ALGORITHM=HS256
JWT_EXPIRATION_MINUTES=15

# ML Models
YOLO_SEGMENTATION_MODEL=yolov11m-seg.pt
YOLO_DETECTION_MODEL=yolov11m.pt

# Application
DEBUG=true
LOG_LEVEL=DEBUG
ENVIRONMENT=development
